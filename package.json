{
  "name": "tic-tac-toe-natac",
  "version": "0.1.0",
  "description": "challenge from Free Code Camp",
  "main": "server.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register test/*.js",
    "postinstall": "webpack -p --config ./webpack.production.config.js --progress --colors",
    "start": "babel-node server.js",
    "x": "babel-node app/js/makeGame.js",
    "jade": "jade -w -P ./app/jade/index.jade -o ./public/",
    "production": "NODE_ENV=production webpack -p --config ./webpack.production.config.js",
    "sim": "NODE_ENV=production webpack -p --config ./webpack.production.config.js && NODE_ENV=production babel-node bin/www"
  },
  "keywords": [
    "javascript",
    "tic-tac-toe"
  ],
  "author": "Sean Campbell",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "chai": "^3.4.1",
    "eslint-plugin-react": "^3.10.0",
    "jade": "^1.11.0",
    "mocha": "^2.3.4",
    "redux-devtools": "^2.1.5",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.1.18",
    "css-loader": "^0.23.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "immutable": "^3.7.5",
    "lodash": "^3.10.1",
    "node-sass": "^3.4.2",
    "path": "^0.12.7",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.8"
  }
}
